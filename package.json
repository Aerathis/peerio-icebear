{
  "name": "peerio-icebear",
  "version": "1.0.0",
  "description": "Peerio Icebear client library",
  "main": "src/index.js",
  "scripts": {
    "prepush": "npm run validate",
    "postcheckout": "opt --out npm-update --exec \"npm update\"",
    "postmerge": "opt --out npm-update --exec \"npm update\"",
    "precommit": "opt --out lint --exec \"npm run lint\"",
    "commitmsg": "validate-commit-msg",
    "mocha": "mocha test/**/*.test.js --require test/global-setup --compilers js:babel-register -R progress --check-leaks --timeout 15000",
    "start": "npm run mocha -- -w",
    "test": "nyc --reporter=text --reporter=html npm run mocha",
    "validate": "npm-run-all --serial lint",
    "lint": "eslint . --fix --ext .js,.jsx",
    "release": "standard-version && git push --follow-tags origin master",
    "doc:serve": "documentation serve -w src/**",
    "doc:lint": "documentation lint src/**",
    "doc:build": "documentation build src/** -f md -o API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "peerDependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.5.0",
    "buffer": "5.0.6",
    "eventemitter3": "2.0.3",
    "lodash": "4.17.4",
    "mobx": "3.1.9",
    "scrypt-async": "1.3.1",
    "socket.io-client": "1.7.4",
    "moment": "2.18.1",
    "tweetnacl": "1.0.0-rc.1",
    "peerio-translator": "git+https://github.com/PeerioTechnologies/peerio-translator.git#icebear"
  },
  "dependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.5.0",
    "buffer": "5.0.6",
    "dompurify": "0.8.9",
    "eventemitter3": "2.0.3",
    "lodash": "4.17.4",
    "mobx": "3.1.9",
    "moment": "2.18.1",
    "peerio-translator": "git+https://github.com/PeerioTechnologies/peerio-translator.git#icebear",
    "scrypt-async": "1.3.1",
    "socket.io-client": "1.7.4",
    "tweetnacl": "1.0.0-rc.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-register": "6.24.1",
    "babel-types": "6.24.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "cz-conventional-changelog": "2.0.0",
    "documentation": "4.0.0-rc.1",
    "eslint": "3.19.0",
    "eslint-config-peerio": "*",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "7.0.0",
    "husky": "0.13.3",
    "mocha": "3.3.0",
    "mock-require": "2.0.2",
    "npm-run-all": "4.0.2",
    "nyc": "10.3.2",
    "opt-cli": "1.5.1",
    "rimraf": "2.6.1",
    "sinon": "2.2.0",
    "sinon-chai": "2.10.0",
    "source-map-support": "0.4.15",
    "standard-version": "4.0.0",
    "validate-commit-msg": "2.12.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "devEngines": {
    "node": "7.x",
    "npm": "4.x"
  }
}
