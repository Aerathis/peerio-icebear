{
  "name": "peerio-icebear",
  "version": "1.0.0",
  "description": "Peerio Icebear client library",
  "main": "src/index.js",
  "scripts": {
    "test": "BABEL_ENV=coverage karma start",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "validate": "npm-run-all --parallel lint  test --serial check-coverage",
    "start": "karma start --no-single-run",
    "lint": "eslint . --fix --ext .js,.jsx",
    "release": "standard-version && git push --follow-tags origin master",
    "doc:serve": "documentation serve -w src/**",
    "doc:lint": "documentation lint src/**",
    "doc:build": "documentation build src/** -f md -o API.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "peerDependencies": {
    "blake2s-js": "1.2.0",
    "bluebird": "3.4.6",
    "buffer": "5.0.0",
    "is_js": "0.9.0",
    "mobx": "2.6.0",
    "mobx-react": "3.5.8",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.5.1",
    "tweetnacl": "0.14.3"
  },
  "dependencies": {
    "blake2s-js": "1.2.0",
    "bluebird": "3.4.6",
    "buffer": "5.0.0",
    "is_js": "0.9.0",
    "mobx": "2.6.0",
    "mobx-react": "3.5.8",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.5.1",
    "tweetnacl": "0.14.3"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.18.0",
    "babel-plugin-transform-es2015-template-literals": "6.8.0",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "diff2html-cli": "2.0.3",
    "documentation": "4.0.0-beta11",
    "eslint-config-peerio": "*",
    "eslint-loader": "1.6.0",
    "file-loader": "0.9.0",
    "ghooks": "1.3.2",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.2.0",
    "karma-nyan-reporter": "0.2.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mobx-logger": "0.3.1",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "rimraf": "2.5.4",
    "standard-version": "^3.0.0",
    "validate-commit-msg": "2.8.2",
    "webpack": "2.1.0-beta.25"
  },
  "config": {
    "ghooks": {
      "pre-commit": "",
      "pre-push": "if [ `git rev-parse --abbrev-ref HEAD` == 'master' ]; then npm run validate; fi",
      "post-checkout": "npm update",
      "post-merge": "npm update",
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
