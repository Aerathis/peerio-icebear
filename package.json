{
  "name": "peerio-icebear",
  "version": "1.0.0",
  "description": "Peerio Icebear client library",
  "main": "src/index.js",
  "scripts": {
    "prepush": "npm run validate",
    "postcheckout": "npm update && npm run ctags",
    "postmerge": "npm update && npm run ctags",
    "precommit": "npm run lint && npm run ctags",
    "postcommit": "npm run ctags",
    "commitmsg": "validate-commit-msg",
    "mocha": "mocha test/**/*.test.js --require test/global-setup --compilers js:babel-register -R progress --check-leaks",
    "start": "npm run mocha -- -w",
    "test": "nyc --reporter=text --reporter=html npm run mocha",
    "validate": "npm-run-all --serial lint",
    "lint": "eslint . --fix --ext .js,.jsx",
    "release": "standard-version && git push --follow-tags origin master",
    "doc:serve": "documentation serve -w src/**",
    "doc:lint": "documentation lint src/**",
    "doc:build": "documentation build src/** -f md -o API.md",
    "ctags": "if [ -f .git/hooks/ctags-all ]; then .git/hooks/ctags-all; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "peerDependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.4.7",
    "buffer": "5.0.5",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.4",
    "mobx": "3.1.0",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.7.2",
    "tweetnacl": "0.14.5"
  },
  "dependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.4.7",
    "buffer": "5.0.5",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.4",
    "mobx": "3.1.0",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.7.2",
    "tweetnacl": "0.14.5"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-register": "6.23.0",
    "babel-types": "6.22.0",
    "chai": "3.5.0",
    "clear-require": "2.0.0",
    "cz-conventional-changelog": "1.2.0",
    "documentation": "4.0.0-beta.18",
    "eslint-config-peerio": "*",
    "husky": "0.13.1",
    "mocha": "3.2.0",
    "mock-require": "2.0.1",
    "npm-run-all": "4.0.1",
    "nyc": "10.1.2",
    "rimraf": "2.5.4",
    "sinon": "2.0.0-pre.6",
    "standard-version": "4.0.0",
    "validate-commit-msg": "2.11.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "devEngines": {
    "node": "7.x",
    "npm": "4.x"
  }
}
