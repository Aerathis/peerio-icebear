{
  "name": "peerio-icebear",
  "version": "1.0.0",
  "description": "Peerio Icebear client library",
  "main": "src/index.js",
  "scripts": {
    "prepush": "if [ `git rev-parse --abbrev-ref HEAD` == 'master' ]; then npm run validate; fi",
    "postcheckout": "npm update && npm run ctags",
    "postmerge": "npm update && npm run ctags",
    "precommit": "npm run lint && npm run ctags",
    "postcommit": "npm run ctags",
    "commitmsg": "validate-commit-msg",
    "test": "BABEL_ENV=coverage karma start",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "validate": "npm-run-all --serial lint",
    "start": "karma start --no-single-run",
    "lint": "eslint . --fix --ext .js,.jsx",
    "release": "standard-version && git push --follow-tags origin master",
    "doc:serve": "documentation serve -w src/**",
    "doc:lint": "documentation lint src/**",
    "doc:build": "documentation build src/** -f md -o API.md",
    "ctags": "if [ -f .git/hooks/ctags-all ]; then .git/hooks/ctags-all; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "peerDependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.4.7",
    "buffer": "5.0.5",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.4",
    "mobx": "3.1.0",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.7.2",
    "tweetnacl": "0.14.5"
  },
  "dependencies": {
    "blake2s-js": "1.2.2",
    "bluebird": "3.4.7",
    "buffer": "5.0.5",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.4",
    "mobx": "3.1.0",
    "nacl-stream": "0.3.3",
    "scrypt-async": "1.3.0",
    "socket.io-client": "1.7.2",
    "tweetnacl": "0.14.5"
  },
  "devDependencies": {
    "babel-cli": "6.22.2",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-class-properties": "6.22.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.22.0",
    "babel-plugin-transform-es2015-template-literals": "6.22.0",
    "babel-plugin-transform-inline-environment-variables": "6.8.0",
    "babel-plugin-transform-object-rest-spread": "6.22.0",
    "babel-plugin-transform-react-jsx": "6.22.0",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-types": "6.22.0",
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "diff2html-cli": "2.0.5",
    "documentation": "4.0.0-beta.18",
    "eslint-config-peerio": "*",
    "eslint-loader": "1.6.1",
    "file-loader": "0.10.0",
    "husky": "0.13.1",
    "istanbul": "0.4.5",
    "json-loader": "0.5.4",
    "karma": "1.4.1",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "mobx-logger": "0.4.0",
    "mocha": "3.2.0",
    "npm-run-all": "4.0.1",
    "rimraf": "2.5.4",
    "standard-version": "4.0.0",
    "validate-commit-msg": "2.11.1",
    "webpack": "2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "devEngines": {
    "node": "6.x",
    "npm": "3.x"
  }
}
