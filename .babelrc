{
    "compact": false,
    "plugins": [
        "transform-decorators-legacy",
        "transform-class-properties",
        "transform-es2015-modules-commonjs",
        "transform-export-extensions",
        "transform-async-to-bluebird",
        [
            "transform-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ]
    ],
    "env": {
        "production": {
            "plugins": [
                "console-kungfu"
            ]
        }
    }
}
