{
  "name": "peerio-icebear",
  "version": "5.12.0",
  "description": "Peerio Icebear client library",
  "main": "dist/index.js",
  "scripts": {
    "=== HOOKS ===": "",
    "postinstall": "[ `which opt` ] && opt --in npm-install --exec \"node ./scripts/store_package_json_hash.js\" || true",
    "postcheckout": "npm run opt-in-npm-install",
    "postmerge": "npm run opt-in-npm-install",
    "precommit": "opt --in lint --exec \"npm run validate\"",
    "commitmsg": "validate-commit-msg",
    "=== TESTS ===": "",
    "test-build": "rm -rf dist && ./node_modules/.bin/babel src --out-dir dist --source-maps --extensions \".js,.ts\"",
    "mocha": "mocha \"test/**/*.test.js\" --require test/unit/unit-global-setup --timeout 15000",
    "test": "run-s test:ts test:unit test:e2e",
    "test:ts": "tsc --noEmit",
    "test:unit": "npm run test-build && nyc --reporter=text --reporter=html --report-dir=test-results/unit  npm run mocha",
    "test:e2e": "./test/e2e/run.sh",
    "test:e2e:wip": "./test/e2e/run-wip.sh",
    "test:e2e:debug": "./test/e2e/debug.sh",
    "=== VALIDATION ===": "",
    "validate": "run-s -l validate:* test:ts",
    "validate:eslint": "eslint '{src,test}/**/*.js' --color --max-warnings 0",
    "validate:eslint-ts": "eslint  -c '.eslintrc.typescript-temp.json' '{src,test}/**/*.ts' --color --max-warnings 0",
    "validate:lint-copy": "./scripts/lint-copy.sh",
    "validate:prettier": "prettier '{src,test}/**/*.{js,ts}' -l",
    "=== BENCHMARKS ===": "",
    "bench": "node ./benchmarks/index",
    "=== RELEASE ===": "",
    "release": "./scripts/release.sh",
    "=== UTILS ===": "",
    "prettier": "prettier '{src,test}/**/*.{js,ts}' --write",
    "opt-in-npm-install": "opt --in npm-install --exec \"node ./scripts/npm_install_if_changed.js\""
  },
  "files": [
    "src",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PeerioTechnologies/peerio-icebear.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PeerioTechnologies/peerio-icebear/issues"
  },
  "homepage": "https://github.com/PeerioTechnologies/peerio-icebear#readme",
  "dependencies": {
    "blake2s-js": "1.2.3",
    "bluebird": "3.5.1",
    "buffer": "5.0.8",
    "eventemitter3": "3.1.0",
    "lodash": "4.17.10",
    "mkdirp": "0.5.1",
    "lru-cache": "4.1.3",
    "mobx": "3.5.1",
    "moment": "2.22.2",
    "peerio-translator": "github:PeerioTechnologies/peerio-translator#v1.0.0",
    "rimraf": "2.6.2",
    "safe-json-stringify": "1.2.0",
    "scrypt-async": "2.0.0",
    "socket.io-client": "2.1.1",
    "tweetnacl": "1.0.0",
    "url-regex": "4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.54",
    "@babel/core": "7.0.0-beta.54",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.54",
    "@babel/plugin-proposal-decorators": "7.0.0-beta.54",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.54",
    "@babel/plugin-transform-async-to-generator": "7.0.0-beta.54",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.54",
    "@babel/preset-typescript": "7.0.0-beta.54",
    "@babel/register": "7.0.0-beta.54",
    "@types/bluebird": "3.5.23",
    "@types/bluebird-global": "3.5.6",
    "@types/node": "10.5.2",
    "babel-eslint": "8.2.6",
    "babel-plugin-root-import": "6.1.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "cucumber": "4.2.1",
    "cucumber-pretty": "1.4.4",
    "cz-conventional-changelog": "2.1.0",
    "eslint-config-peerio": "1.21.7",
    "eslint-config-prettier": "2.9.0",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "opt-cli": "1.6.0",
    "otplib": "10.0.0",
    "prettier": "1.13.7",
    "quoted-printable": "1.0.1",
    "request": "2.87.0",
    "standard-version": "4.4.0",
    "typescript": "3.0.1",
    "typescript-eslint-parser": "17.0.1",
    "validate-commit-msg": "2.14.0",
    "w3c-xmlhttprequest": "2.1.2",
    "websocket": "1.0.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 120,
      "subjectPattern": ".+",
      "helpMessage": "Convention: https://goo.gl/3TgTsh"
    }
  },
  "standard-version": {
    "scripts": {
      "prebump": "node ./scripts/prebump.js",
      "postbump": "node ./scripts/postbump.js"
    }
  },
  "devEngines": {
    "node": "8.x",
    "npm": "5.x"
  }
}
